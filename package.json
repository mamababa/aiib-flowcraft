        "@types/react-intl": "2.2.2",
        "@types/react-router": "3.0.0",
        "@types/redux-actions": "^1.2.7",
        "@types/sharepoint": "^2013.1.2",
        "@types/superagent": "2.0.36",
        "awesome-typescript-loader": "^3.2.2",
        "cross-env": "3.1.3",
        "css-loader": "0.26.1",
