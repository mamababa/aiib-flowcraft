{
    "name": "aiibworkflow",
    "version": "1.1.13",
    "description": "AIIB Workflow",
    "main": "app.tsx",
    "scripts": {
        "build": "cross-env NODE_ENV=locale webpack  --colors  --sort-modules-by size --config webpack.config.js",
        "dev": "cross-env NODE_ENV=dev webpack  --colors  --sort-modules-by size --config webpack.config.js",
        "uat": "cross-env NODE_ENV=uat webpack  --colors  --sort-modules-by size --config webpack.config.js",
        "aiib": "cross-env NODE_ENV=aiib webpack  --colors  --sort-modules-by size --config webpack.config.js",
        "aiibdev": "cross-env NODE_ENV=aiibdev webpack  --colors  --sort-modules-by size --config webpack.config.js",
        "aiibuat": "cross-env NODE_ENV=aiibuat webpack  --colors  --sort-modules-by size --config webpack.config.js",
        "aiibpub": "cross-env NODE_ENV=aiibpub webpack  --colors  --sort-modules-by size --config webpack.config.js",
        "pub": "cross-env NODE_ENV=production  webpack --colors  --sort-modules-by size --config webpack.config.js",
        "dll": "cross-env NODE_ENV=uat  webpack  --config ./config/webpack.dll.config.js",
        "start": "cross-env NODE_ENV=development webpack-dev-server --https --host 127.0.0.1 --inline --colors --profile --config ./config/webpack.dev.config.js",
        "local": "cross-env NODE_ENV=local webpack-dev-server --hot  --host 0.0.0.0 --inline --colors --profile --config ./config/webpack.dev.config.js",
        "prepack": "cross-env NODE_ENV=dev webpack  --colors  --sort-modules-by size --config ./config/webpack.prepack.config.js"
    },
    "repository": {
        "type": "git",
        "url": "http://tfs-sh.yeeoffice.cn:8090/tfs/DefaultCollection/akmii-yeeoffice-fe/_git/akmii-yeeoffice-flowcraft"
    },
    "keywords": [
        "ts",
        "react",
        "antd"
    ],
    "author": "akmii",
    "license": "ISC",
    "dependencies": {
        "akmii-yeeoffice-common": "git+http://tfs-sh.yeeoffice.cn:8090/tfs/DefaultCollection/akmii-yeeoffice-fe/_git/akmii-yeeoffice-common-dist#aiib-workflow-optimize",
        "akmii-yeeoffice-crafts": "git+http://tfs-sh.yeeoffice.cn:8090/tfs/DefaultCollection/akmii-yeeoffice-fe/_git/akmii-yeeoffice-crafts-dist#aiib-workflow-optimize",
